{
  "loader_config": {
    "loader_type": "docred"
  },
  "train_dataset_path": "etc/datasets/docred/train_annotated.json",
  "dev_dataset_path": "etc/datasets/docred/dev.json",
  "eval_dataset_path": "etc/datasets/docred/dev.json",
  "pred_dataset_path": "etc/datasets/docred/test.json",
  "training_config": {
    "compute_metrics": false,
    "training_arguments": {
      "num_train_epochs": 30,
      "evaluation_strategy": "no",
      "save_total_limit": 1,
      "do_train": true,
      "do_eval": true,
      "no_cuda": false,
      "per_device_train_batch_size": 4,
      "per_device_eval_batch_size": 8,
      "warmup_ratio": 0.06
    }
  },
  "extra_training_config": {
    "encoder_lr": 3e-5,
    "learning_rate": 5e-4,
    "adam_epsilon": 1e-6
  },
  "train_diversifier": {
    "replace_prob": 0.85,
    "noise_prob": 0.15,
    "mapping": {
      "ORG": ["ORG", "INSTITUTE", "GROUPING"],
      "LOC": ["LOC", "AREA", "DISTRICT"],
      "TIME": ["TIME", "MOMENT", "STAGE"],
      "PER": ["PER", "CHARACTER", "HUMAN"],
      "MISC": ["MISC", "ETC", "DIFFERENT"],
      "NUM": ["NUM", "DIGIT", "QUANTITY"]
    }
  },
  "dev_diversifier": {
    "replace_prob": 0.85,
    "noise_prob": 0.15,
    "mapping": {
      "ORG": ["ORG", "INSTITUTE", "GROUPING"],
      "LOC": ["LOC", "AREA", "DISTRICT"],
      "TIME": ["TIME", "MOMENT", "STAGE"],
      "PER": ["PER", "CHARACTER", "HUMAN"],
      "MISC": ["MISC", "ETC", "DIFFERENT"],
      "NUM": ["NUM", "DIGIT", "QUANTITY"]
    }
  },
  "eval_diversifier": {
    "replace_prob": 1,
    "mapping": {
      "ORG": ["ESTABLISHMENT"],
      "LOC": ["POSITION"],
      "TIME": ["DAY"],
      "PER": ["INDIVIDUAL"],
      "MISC": ["VARIOUS"],
      "NUM": ["SIGN"]
    }
  },
  "test_diversifier": {
     "replace_prob": 1,
    "mapping": {
      "ORG": ["ORGANIZATION"],
      "LOC": ["LOCATION"],
      "TIME": ["DATE"],
      "PER": ["PERSON"],
      "MISC": ["MISCELLANEOUS"],
      "NUM": ["NUMBER"]
    }
  },
  "pred_diversifier": {
    "replace_prob": 1,
    "mapping": {
      "ORG": ["ORGANIZATION"],
      "LOC": ["LOCATION"],
      "TIME": ["DATE"],
      "PER": ["PERSON"],
      "MISC": ["MISCELLANEOUS"],
      "NUM": ["NUMBER"]
    }
  },
