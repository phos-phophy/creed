{
  "loader_config": {
    "loader_type": "docred"
  },
  "init_config": {
    "load_path": "etc/models/ssan_adapt/docred/distant_base/model.pkl"
  },
  "training_config": {
    "compute_metrics": false,
    "training_arguments": {
      "logging_dir": "etc/models/ssan_adapt/docred/base/log_dir",
      "output_dir": "etc/models/ssan_adapt/docred/base/output",
      "num_train_epochs": 60,
      "save_total_limit": 1,
      "evaluation_strategy": "epoch",
      "do_train": true,
      "do_eval": true,
      "no_cuda": false,
      "per_device_train_batch_size": 5,
      "per_device_eval_batch_size": 5,
      "warmup_ratio": 0.1,
      "learning_rate": 0.00003
    }
  },
  "train_dataset_path": "etc/datasets/docred/train_annotated.json",
  "dev_dataset_path": "etc/datasets/docred/dev.json",
  "test_dataset_path": "etc/datasets/docred/test.json",
  "save_path": "etc/models/ssan_adapt/docred/base/model.pkl",
  "output_eval_path": "etc/models/ssan_adapt/docred/base/eval_results.json",
  "output_pred_path": "etc/models/ssan_adapt/docred/base/result.json"
}
