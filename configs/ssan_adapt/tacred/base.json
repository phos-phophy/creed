      "output_dir": "etc/models/ssan_adapt/tacred/base/output",
      "num_train_epochs": 60,
      "evaluation_strategy": "epoch",
      "save_total_limit": 1,
      "do_train": true,
      "do_eval": true,
      "no_cuda": false,
      "per_device_train_batch_size": 5,
      "per_device_eval_batch_size": 5,
      "warmup_ratio": 0.1,
      "learning_rate": 0.00003
